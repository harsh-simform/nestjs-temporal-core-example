{
  "name": "nestjs-temporal-core-example",
  "version": "1.0.0",
  "description": "A comprehensive example application demonstrating nestjs-temporal-core package with order management workflow",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "temporal:up": "docker-compose up -d",
    "temporal:down": "docker-compose down",
    "temporal:logs": "docker-compose logs -f temporal",
    "test:order": "./test-order.sh"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@temporalio/client": "^1.8.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "nestjs-temporal-core": "file:.yalc/nestjs-temporal-core",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
